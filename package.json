{
  "name": "shumskaya",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"src/**/*.{js,ts,tsx,jsx}\" --fix",
    "prepare": "husky install",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check \"src/**/*.{js,ts,tsx,jsx}\"",
    "check-lint": "eslint .",
    "format": "prettier --write \"src/**/*.{js,ts,tsx,jsx}\"",
    "test-all": "pnpm run check-format && pnpm run check-lint && pnpm run check-types && pnpm run build"
  },
  "dependencies": {
    "@devnomic/marquee": "^1.0.2",
    "@radix-ui/react-slot": "^1.1.1",
